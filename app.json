{
  "expo": {
    "name": "Pocket Player",
    "slug": "pocket-player",
    "scheme": "pocket-player",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ios_app_logo_marketing.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#09090b",
    "primaryColor": "#0d968b",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#09090b"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#09090b"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#09090b"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/web_favicon.ico",
      "icons": [
        {
          "src": "./assets/icons/web_logo_192.png",
          "sizes": "192x192",
          "type": "image/png"
        },
        {
          "src": "./assets/icons/web_logo_512.png",
          "sizes": "512x512",
          "type": "image/png"
        },
        {
          "src": "./assets/icons/web_logo_192_maskable.png",
          "sizes": "192x192",
          "type": "image/png",
          "purpose": "maskable"
        },
        {
          "src": "./assets/icons/web_logo_512_maskable.png",
          "sizes": "512x512",
          "type": "image/png",
          "purpose": "maskable"
        },
        {
          "src": "./assets/icons/web_apple_touch_logo.png",
          "sizes": "180x180",
          "type": "image/png",
          "purpose": "apple-touch-icon"
        }
      ]
    },
    "ios": {
      "icon": "./assets/icons/ios_app_logo_marketing.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.pocketplayer",
      "backgroundColor": "#09090b"
    },
    "android": {
      "package": "com.anonymous.pocketplayer",
      "edgeToEdgeEnabled": true,
      "icon": "./assets/icons/android_adaptive_logo_foreground.png",
      "backgroundColor": "#09090b",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android_adaptive_logo_foreground.png",
        "backgroundColor": "#09090b"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "softwareKeyboardLayoutMode": "pan"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-sqlite",
      "expo-video",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      "expo-web-browser",
      "./plugins/flag-secure.js",
      "./plugins/custom-app-theme.js",
      "./plugins/custom-colors.js"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "24f3ef50-04cd-4c54-bb3c-598d81de285d"
      }
    },
    "owner": "huffmanks"
  }
}
