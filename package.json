    "expo-local-authentication": "^14.0.1",
    "expo-media-library": "^16.0.5",
    "expo-navigation-bar": "^3.0.7",
    "expo-router": "~3.5.24",
    "expo-sqlite": "^14.0.6",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
