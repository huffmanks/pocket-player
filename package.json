  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react-native-svg",
        "@urql/core",
        "vite"
      ],
      "overrides": {
        "react-native-svg": "15.2.0"
      }
    }
  },
  "private": true
