{
  "name": "video-player-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start --dev-client --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start": "expo start",
    "prebuild": "expo prebuild --platform android --clean",
    "build:dev": "NODE_ENV=development eas build --platform android --profile development --local",
    "build:preview": "NODE_ENV=development eas build --platform android --profile preview --local",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "db:generate": "drizzle-kit generate"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
